<h1>Search Results</h1>

<% if @societies.nil? %>
<% else %>
    <table>
      <thead>
      <tr>
        <th>Name</th>
        <th>Website</th>
        <th>Approved</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @societies.each do |society| %>
          <tr>
            <td><%= society.name %></td>
            <td><%= society.website %></td>
            <td><%= society.approved %></td>
            <td><%= link_to 'Show', society %></td>
          </tr>
      <% end %>
      </tbody>
    </table>
<% end %>

<% if @events.nil? %>
<% else %>
<table>
  <thead>
  <tr>
    <th>Name</th>

    <th>Type</th>
    <th>Description</th>
    <th>Date</th>
    <th>Time</th>
    <th>[to fix] Starttime</th>
    <th>[to fix] Endtime</th>
    <th>[todo] Hosted By</th>
    <th>[not doing?] Website</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.eventType %></td>
        <td><%= event.description %></td>
        <td><%= event.eventDate %></td>
        <td><%= event.eventTime %></td>
        <!-- event.startTime event.endTime
        -->
        <td><%= link_to 'Show', event %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<% end %>