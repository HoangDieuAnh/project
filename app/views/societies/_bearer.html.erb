      <%=s.fields_for :relationships, @society.get_relationships do |r|%>
      <div class="form-group">
      	<label> Office Bearer details</label>
      <%= r.text_field :position, class: "form-control input-lg", placeholder: 'Position' %>
      </div>
      <div class="form-group">
      <%= r.text_field :firstname, class: "form-control input-lg", placeholder: 'First Name' %>
      </div>
      <div class="form-group">
        <%= r.text_field :middlename, class: "form-control input-lg", placeholder: 'Middle Name' %>
      </div>
      <div class="form-group">
        <%= r.text_field :lastname, class: "form-control input-lg", placeholder: 'Last Name' %>
      </div>
      <div class="form-group">
        <%= r.email_field :email, class: "form-control input-lg", placeholder: 'University Email' %>
      </div>
      <div class="form-group">
        <%= r.phone_field :phone, class: "form-control input-lg", placeholder: 'Phone' %>
      </div>
      <%end%>
